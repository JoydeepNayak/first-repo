{
  "name": "@infoserver/gov-shared-ui",
  "version": "0.2.15",
  "_inBundle": false,
  "description": "Shared governance components",
  "files": [
    "dist",
    "lib",
    "docs"
  ],
  "main": "./dist/gov-shared-ui.js",
  "module": "./lib",
  "scripts": {
    "dev": "cross-env NODE_ENV=dev-server webpack-dev-server",
    "build": "cross-env NODE_ENV=dev webpack --progress",
    "build:single": "webpack -p --config webpack.single.js",
    "dist": "npm run build:clean && npm run build && npm run build:single",
    "watch": "cross-env NODE_ENV=dev webpack --watch",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "lint:eslint": "eslint --ignore-path .gitignore",
    "lint": "npm run lint:eslint -- . ",
    "build:clean": "rimraf ./build && rimraf ./dist && rimraf ./lib",
    "build:full": "npm run build:clean && npm run lint && npm run build && jest --runInBand "
  },
  "author": "IBM",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://github.ibm.com/InformationServer/gov-shared-ui"
  },
  "bundleDependencies": false,
  "dependencies": {
    "ap-icons": "^1.2.3",
    "carbon-components": "^7.22.3",
    "carbon-components-react": "^3.12.2",
    "carbon-icons": "^6.1.0",
    "copy-webpack-plugin": "^4.0.1",
    "flatpickr": "^3.0.7",
    "html-webpack-plugin": "^2.30.1",
    "ibm-design-icons": "^2.1.3",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "react-draggable": "^2.2.6",
    "react-grid-layout": "^0.14.7",
    "react-redux": "^5.0.6",
    "react-router": "^3.0.2"
  },
  "devDependencies": {
    "ap-components-react": "^3.5.2",
    "babel-cli": "^6.24.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-react-intl": "^2.3.1",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-exponentiation-operator": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.24.1",
    "classnames": "^2.2.5",
    "color-difference": "^0.3.4",
    "cross-env": "^5.0.1",
    "css-loader": "^0.26.2",
    "d3": "^4.10.0",
    "enzyme": "^2.8.2",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "15.0.0",
    "eslint-config-airbnb-base": "11.2.0",
    "eslint-import-resolver-webpack": "0.8.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "5.0.1",
    "eslint-plugin-react": "7.0.1",
    "fetch-mock": "^5.11.0",
    "ibm-design-colors": "^2.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^20.0.4",
    "jest-fetch-mock": "^1.2.1",
    "node-sass": "^4.5.3",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-addons-test-utils": "^15.6.0",
    "react-dom": "^15.6.1",
    "react-modal": "^2.2.2",
    "redbox-react": "^1.4.3",
    "redux": "^3.6.0",
    "redux-mock-store": "^1.2.3",
    "redux-thunk": "2.2.0",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "sinon": "^2.3.5",
    "style-loader": "^0.13.2",
    "svg-url-loader": "^2.1.1",
    "webpack": "^3.4.1",
    "webpack-dev-server": "^2.7.1"
  },
  "peerDependencies": {
    "d3": "4.x",
    "prop-types": "15.x",
    "react": "15.x",
    "react-dom": "15.x",
    "ap-components-react": "3.x"
  },
  "jest": {
    "setupFiles": [
      "./jestSetup.js"
    ],
    "roots": [
      "src"
    ],
    "moduleDirectories": [
      "node_modules",
      "src/components"
    ],
    "moduleNameMapper": {
      "\\.(svg)$": "<rootDir>/test/mocks/staticFileMock.js",
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/components/**/*.js"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 80,
        "branches": 80,
        "functions": 80,
        "lines": 80
      }
    },
    "coverageDirectory": "build/coverage"
  }
}
